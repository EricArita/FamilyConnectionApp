<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.familyconnectionapp.ui.myLocation.MyLocationFragment"
        android:label="@string/menu_myLocation"
        tools:layout="@layout/fragment_my_location" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.familyconnectionapp.ui.myCircle.MyCircleFragment"
        android:label="@string/menu_myCircle"
        tools:layout="@layout/fragment_my_circle" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.familyconnectionapp.ui.joinCircle.JoinCircleFragment"
        android:label="@string/menu_joinCircle"
        tools:layout="@layout/fragment_join_circle" />

    <fragment
        android:id="@+id/nav_tools"
        android:name="com.example.familyconnectionapp.ui.tools.ToolsFragment"
        android:label="@string/menu_joinedCircle"
        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.example.familyconnectionapp.ui.joinedCircle.JoinedCircleFragment"
        android:label="@string/menu_signOut"
        tools:layout="@layout/fragment_joined_circle" />
</navigation>